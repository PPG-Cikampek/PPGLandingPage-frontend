{
  "kind": "collectionType",
  "collectionName": "content_managers",
  "info": {
    "singularName": "content-manager",
    "pluralName": "content-managers",
    "displayName": "Content Manager"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string"
    },
    "branch": {
      "type": "string"
    },
    "subBranch": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "author/writer",
        "editor/publisher"
      ]
    },
    "createdArticles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "author"
    },
    "publishedArticles": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::article.article",
      "mappedBy": "publisher"
    }
  }
}
