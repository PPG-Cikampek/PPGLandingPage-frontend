{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "address": {
      "type": "customField",
      "customField": "plugin::strapi-location-picker.location-picker",
      "options": {
        "info": true
      }
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": true
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content-manager.content-manager",
      "inversedBy": "createdArticles"
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::content-manager.content-manager",
      "inversedBy": "publishedArticles"
    },
    "viewCount": {
      "type": "integer"
    }
  }
}
